{
  "name": "stanks-utilities-node",
  "version": "1.0.0",
  "description": "Small collection of operations that I frequently use.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "lint": "tsc --noEmit && eslint ./src --ext .ts",
    "start": "npm run prod",
    "server": "babel-node ./src/index --extensions .ts",
    "server:prod": "node ./dist/index",
    "dev": "cross-env NODE_ENV=development && npm-run-all lint server",
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist --extensions .ts",
    "prod": "cross-env NODE_ENV=production && npm-run-all lint clean build server:prod",
    "watch": "nodemon"
  },
  "author": "Kyle Stankovich",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  }
}
