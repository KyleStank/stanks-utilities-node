{
  "name": "stanks-utilities-node",
  "version": "1.0.2",
  "description": "Small collection of operations that I frequently use.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "jsnext:main": "./lib/index.es.js",
  "types": "./@types/index.d.ts",
  "files": [
    "/lib",
    "/@types"
  ],
  "scripts": {
    "build": "npm-run-all clean lint build:rollup",
    "build:rollup": "rollup -c",
    "clean": "rimraf lib",
    "lint": "tsc --noEmit && eslint ./src ./@types --ext .ts,.d.ts",
    "test": "echo \"Error: no test specified\"",
    "watch": "nodemon --exec \"npm run lint\" --watch ./src --ext .ts,.d.ts"
  },
  "author": "Kyle Stankovich",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/KyleStank/stanks-utilities-node.git"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.5"
  }
}
